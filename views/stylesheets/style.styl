linkcolor = brown - 25%
hovercolor = brow
bgcolor = white - 10%
tablebordersize = 2px
body
  margin 100px auto
  // background white 20%
  background bgcolor
  color black + 10%
  font 16px Futura, "Lucida Grande", helvetica, sans-serif
  width 500px
#header
  padding 50px
  background-color bgcolor - 30%
#wrapper
  text-align left
  li
    display block
  a
    color linkcolor
    font-weight bold
  a:hover
    color hovercolor
    font-weight bold
  h1
    color brown - 50%
#content
  line-height 24px
  padding 50px
#footer
  text-align center
  font-size 10px

//table formating
table
  // border 1px solid brown
th
  border tablebordersize solid brown
tr
  text-align left
  border tablebordersize solid brown
td
  border tablebordersize solid brown
